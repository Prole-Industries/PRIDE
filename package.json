{
  "name": "PRIDE",
  "version": "1.0.0",
  "description": "Official Prole Industries IDE",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/somecollagist/PRIDE.git"
  },
  "keywords": [],
  "author": "Johann Scott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somecollagist/PRIDE/issues"
  },
  "homepage": "https://github.com/somecollagist/PRIDE#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.3.0"
  },
  "dependencies": {
    "custom-electron-titlebar": "^3.2.6",
    "electron-squirrel-startup": "^1.0.0",
    "monaco-editor-electron": "^1.0.6",
    "monaco-loader": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "PRIDE"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
